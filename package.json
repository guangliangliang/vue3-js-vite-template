{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js", 
  "scripts": {
    "dev": "vite",
    "electron:dev": "rimraf dist-electron && concurrently -k \"vite\" \"wait-on tcp:5173 && electron .\"",
    "build": "vite build",
    "build:electron": "npm run build && electron-builder",
    "preview": "vite preview",
    "format:prettier": "prettier --write \"src/**/*.{js,vue,css,scss}\"",
    "format:stylelint": "stylelint \"src/**/*.{css,scss}\" --fix",
    "format:eslint": "eslint --ext .js,.vue src --fix",
    "format": "npm run format:prettier && npm run format:stylelint && npm run format:eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.11",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "swiper": "^8.4.7",
    "vue": "^3.5.13",
    "vue-awesome-swiper": "^5.0.1",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.4",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.89.1",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.0",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^10.1.3",
    "wait-on": "^8.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,vue,css,scss}": "npm run format"
  },
   "build": {
    "appId": "com.example.myapp",
    "productName": "我的应用",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist",
      "electron"
    ],
    "win": {
      "icon": "electron/assets/icon.png"
    },
    "mac": {
      "icon": "electron/assets/icon.png"
    },
    "linux": {
      "icon": "electron/assets/icon.png"
    }
  }
}
